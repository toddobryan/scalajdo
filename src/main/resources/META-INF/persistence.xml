<?xml version="1.0" encoding="UTF-8" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence 
 http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd" version="2.0">
  <!-- Online Store -->
  <persistence-unit name="scalajdo.examples">
    <class>scalajdo.examples.ImmutableListExample</class>
    <exclude-unlisted-classes/>
    <properties>
      <property name="datanucleus.metadata.validate" value="true"/>
      <property name="datanucleus.ConnectionDriverName" value="org.h2.Driver"/>
      <property name="datanucleus.ConnectionURL" value="jdbc:h2:data;MVCC=true;AUTO_SERVER=true"/>
      <property name="datanucleus.ConnectionUserName" value=""/>
      <property name="datanucleus.ConnectionPassword" value=""/>
      <property name="datanucleus.autoCreateSchema" value="true"/>
    </properties>
  </persistence-unit>
</persistence>